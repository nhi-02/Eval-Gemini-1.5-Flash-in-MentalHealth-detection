    "# --- H√†m g·ªçi API v√† tr·∫£ k·∫øt qu·∫£ ---\n",
    "def get_gemini_prediction(\n",
    "    text_input: str,\n",
    "    prompt_template: str,\n",
    "    model,\n",
    "    max_retries: int = 3, # C√≥ th·ªÉ tƒÉng nh·∫π s·ªë l·∫ßn th·ª≠ l·∫°i\n",
    "    delay: int = 7        # C√≥ th·ªÉ ƒëi·ªÅu ch·ªânh delay\n",
    "    ):\n",
    "    try:\n",
    "        prompt = prompt_template.format(text=text_input)\n",
    "    except KeyError:\n",
    "        print(f\"L·ªói: Template prompt kh√¥ng ch·ª©a placeholder '{{text}}'. Ki·ªÉm tra file CSV.\")\n",
    "        return None, 0 # Tr·∫£ v·ªÅ 0 token n·∫øu l·ªói format\n",
    "    except Exception as format_err:\n",
    "        print(f\"L·ªói ƒë·ªãnh d·∫°ng prompt: {format_err}\")\n",
    "        return None, 0 # Tr·∫£ v·ªÅ 0 token n·∫øu l·ªói format\n",
    "\n",
    "    retries = 0\n",
    "    while retries < max_retries:\n",
    "        try:\n",
    "            safety_settings = {\n",
    "                 genai.types.HarmCategory.HARM_CATEGORY_HARASSMENT: genai.types.HarmBlockThreshold.BLOCK_ONLY_HIGH,\n",
    "                 genai.types.HarmCategory.HARM_CATEGORY_HATE_SPEECH: genai.types.HarmBlockThreshold.BLOCK_ONLY_HIGH,\n",
    "                 genai.types.HarmCategory.HARM_CATEGORY_SEXUALLY_EXPLICIT: genai.types.HarmBlockThreshold.BLOCK_ONLY_HIGH,\n",
    "                 genai.types.HarmCategory.HARM_CATEGORY_DANGEROUS_CONTENT: genai.types.HarmBlockThreshold.BLOCK_ONLY_HIGH,\n",
    "            }\n",
    "            response = model.generate_content(prompt, safety_settings=safety_settings)\n",
    "\n",
    "            # --- L·∫•y th√¥ng tin token ---\n",
    "            total_tokens = 0\n",
    "            try:\n",
    "                 # Ki·ªÉm tra xem usage_metadata c√≥ t·ªìn t·∫°i v√† c√≥ d·ªØ li·ªáu kh√¥ng\n",
    "                 if hasattr(response, 'usage_metadata') and response.usage_metadata:\n",
    "                      prompt_tokens = response.usage_metadata.prompt_token_count\n",
    "                      candidate_tokens = response.usage_metadata.candidates_token_count\n",
    "                      # M·ªôt s·ªë API tr·∫£ v·ªÅ total_token_count tr·ª±c ti·∫øp\n",
    "                      if hasattr(response.usage_metadata, 'total_token_count'):\n",
    "                           total_tokens = response.usage_metadata.total_token_count\n",
    "                      else:\n",
    "                           total_tokens = prompt_tokens + candidate_tokens # T√≠nh t·ªïng n·∫øu kh√¥ng c√≥ total\n",
    "                 else:\n",
    "                      print(f\"C·∫£nh b√°o: Kh√¥ng t√¨m th·∫•y usage_metadata trong ph·∫£n h·ªìi cho: {text_input[:50]}...\")\n",
    "            except AttributeError as ae:\n",
    "                 print(f\"C·∫£nh b√°o: usage_metadata thi·∫øu thu·ªôc t√≠nh token count nh∆∞ mong ƒë·ª£i: {ae}\")\n",
    "            except Exception as token_err:\n",
    "                 print(f\"L·ªói khi l·∫•y token count: {token_err}\")\n",
    "            # -------------------------\n",
    "\n",
    "            prediction_text = response.text.strip().lower()\n",
    "\n",
    "            if \"negative\" in prediction_text:\n",
    "                return \"Negative\", total_tokens # <<< Tr·∫£ v·ªÅ c·∫£ token\n",
    "            elif \"positive\" in prediction_text:\n",
    "                return \"Positive\", total_tokens # <<< Tr·∫£ v·ªÅ c·∫£ token\n",
    "            else:\n",
    "                print(f\"C·∫£nh b√°o: ƒê·ªãnh d·∫°ng output kh√¥ng mong mu·ªën: '{response.text}'. Tr·∫£ v·ªÅ None.\")\n",
    "                return None, total_tokens # <<< Tr·∫£ v·ªÅ token ngay c·∫£ khi format sai\n",
    "\n",
    "        except genai.types.BlockedPromptException as bpe:\n",
    "             print(f\"L·ªói API: Prompt b·ªã ch·∫∑n. L√Ω do: {bpe}. Th·ª≠ l·∫°i ({retries+1}/{max_retries})...\")\n",
    "             retries += 1\n",
    "             time.sleep(delay * (retries + 1)) # TƒÉng delay khi b·ªã ch·∫∑n\n",
    "        except genai.types.StopCandidateException as sce:\n",
    "             print(f\"L·ªói API: N·ªôi dung b·ªã ch·∫∑n trong ph·∫£n h·ªìi. L√Ω do: {sce}. Th·ª≠ l·∫°i ({retries+1}/{max_retries})...\")\n",
    "             retries += 1\n",
    "             time.sleep(delay * (retries + 1)) # TƒÉng delay khi b·ªã ch·∫∑n\n",
    "        except Exception as e:\n",
    "            error_message = str(e)\n",
    "            print(f\"L·ªói API: {error_message}. Th·ª≠ l·∫°i ({retries+1}/{max_retries})...\")\n",
    "            current_delay = delay\n",
    "            if \"429\" in error_message or \"503\" in error_message: # L·ªói rate limit ho·∫∑c server\n",
    "                current_delay = delay * (retries + 2)\n",
    "                print(f\"C√≥ v·∫ª g·∫∑p l·ªói rate limit/server, tƒÉng delay l√™n {current_delay}s\")\n",
    "            retries += 1\n",
    "            time.sleep(current_delay)\n",
    "\n",
    "    print(f\"L·ªói: Kh√¥ng th·ªÉ l·∫•y d·ª± ƒëo√°n cho vƒÉn b·∫£n '{text_input[:50]}...' sau {max_retries} l·∫ßn th·ª≠.\")\n",
    "    return None, 0 # <<< Tr·∫£ v·ªÅ 0 token n·∫øu th·∫•t b·∫°i ho√†n to√†n"
   ]
  },
